utton onClick={onFormSubmit} className=\"ui primary button\">\n            Send\n          </button>\n          <button className=\"ui button\">Cancel</button>\n        </div>\n      \n    </React.Fragment>\n  );\n};\n\n// export default withRouter(PostForm);\nexport default PostForm;\n","/home/dci/Documents/PROJEKTE/REACT-PROJEKTE/react-blog-comment/src/components/AddPost.jsx",[],["62","63"],"/home/dci/Documents/PROJEKTE/REACT-PROJEKTE/react-blog-comment/src/api.js",[],["64","65"],"/home/dci/Documents/PROJEKTE/REACT-PROJEKTE/react-blog-comment/src/components/EditPost.jsx",["66"],"/home/dci/Documents/PROJEKTE/REACT-PROJEKTE/react-blog-comment/src/components/DeleteModal.jsx",[],{"ruleId":"67","replacedBy":"68"},{"ruleId":"69","replacedBy":"70"},{"ruleId":"67","replacedBy":"71"},{"ruleId":"69","replacedBy":"72"},{"ruleId":"73","severity":1,"message":"74","line":37,"column":6,"nodeType":"75","endLine":37,"endColumn":8,"suggestions":"76"},{"ruleId":"77","severity":1,"message":"78","line":3,"column":10,"nodeType":"79","messageId":"80","endLine":3,"endColumn":20},{"ruleId":"67","replacedBy":"81"},{"ruleId":"69","replacedBy":"82"},{"ruleId":"67","replacedBy":"83"},{"ruleId":"69","replacedBy":"84"},{"ruleId":"73","severity":1,"message":"74","line":13,"column":5,"nodeType":"75","endLine":13,"endColumn":7,"suggestions":"85"},"no-native-reassign",["86"],"no-negated-in-lhs",["87"],["86"],["87"],"react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'id'. Either include it or remove the dependency array.","ArrayExpression",["88"],"no-unused-vars","'withRouter' is defined but never used.","Identifier","unusedVar",["86"],["87"],["86"],["87"],["89"],"no-global-assign","no-unsafe-negation",{"desc":"90","fix":"91"},{"desc":"90","fix":"92"},"Update the dependencies array to be: [id]",{"range":"93","text":"94"},{"range":"95","text":"94"},[1017,1019],"[id]",[381,383]]