i\";\nimport { Link, useHistory, useParams} from 'react-router-dom';\nimport DeleteModal from './DeleteModal';\n\n\nconst Post = (props) => {\n  // const { id } = props.match.params;\n  const { id } = useParams();\n  const [post, setPost] = useState({});\n  const [comments, setComments] = useState([]); \n\n  const handleCommentSubmit = (e, comment) => {\n    e.preventDefault();\n    api()\n      .post(`/posts/${id}/comments`, comment)\n      .then((response) => {\n        setComments([...comments, response.data]);\n      })\n      .catch((error) => {\n        window.alert(error);\n      });\n  };\n\n  useEffect(() => {\n    axios\n      .all([api().get(`/posts/${id}`), api().get(`/posts/${id}/comments`)])\n      .then((responses) => {\n        setPost(responses[0].data);\n        setComments(responses[1].data);\n      })\n      .catch((err) => {\n        window.alert(err);\n      });\n  }, []);\n\n  return (\n    <React.Fragment>\n      <h2 className=\"ui header\">{post.title}</h2>\n      <p>{post.created_at}</p>\n      <div className=\"ui buttons\">\n        <Link to={`/posts/${post.id}/edit`} className=\"ui teal button\">Edit Post</Link>\n        {/* <DeleteModal post={post} push={props.history.push}/> */}\n        <DeleteModal post={post} push={useHistory().push}/>\n      </div>\n      <p>{post.content}</p>\n\n      <PostComments\n        comments={comments}\n        handleCommentSubmit={handleCommentSubmit}\n      />\n    </React.Fragment>\n  );\n};\n\nexport default Post;\n","/home/dci/Documents/PROJEKTE/REACT-PROJEKTE/react-blog-comment/src/components/CommentList.jsx",[],["55","56"],"/home/dci/Documents/PROJEKTE/REACT-PROJEKTE/react-blog-comment/src/components/CommentForm.jsx",[],"/home/dci/Documents/PROJEKTE/REACT-PROJEKTE/react-blog-comment/src/components/PostComments.jsx",[],"/home/dci/Documents/PROJEKTE/REACT-PROJEKTE/react-blog-comment/src/components/PostForm.jsx",[],"/home/dci/Documents/PROJEKTE/REACT-PROJEKTE/react-blog-comment/src/components/AddPost.js